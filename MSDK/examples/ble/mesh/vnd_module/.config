CONFIG_BT_MESH=y
CONFIG_BT_MESH_PROXY_MSG_LEN=66
CONFIG_BT_MESH_GATT_CLIENT=y
CONFIG_BT_MESH_GATT_SERVER=y
CONFIG_BT_MESH_PB_GATT_COMMON=y
CONFIG_BT_MESH_PROV=y

#
# Mesh Settings & Testing
#
CONFIG_BT_DEVICE_NAME="GD-BLE-MESH"
# CONFIG_BT_TESTING is not set
# CONFIG_MESH_CB_REGISTERED is not set
CONFIG_BT_SETTINGS=y
# end of Mesh Settings & Testing

#
# BT Conn
#
CONFIG_BT_CONN=y
CONFIG_BT_MAX_CONN=2
# end of BT Conn

#
# Advertiser
#
CONFIG_BT_MESH_ADV_EXT=y
CONFIG_BT_MESH_ADV_STACK_SIZE=1024
CONFIG_BT_MESH_ADV_PRIO=2
CONFIG_BT_EXT_ADV_MAX_ADV_SET=4
# CONFIG_BT_MESH_ADV_EXT_GATT_SEPARATE is not set
CONFIG_BT_MESH_ADV_BUF_COUNT=6
# CONFIG_BT_MESH_DEBUG_USE_ID_ADDR is not set
# end of Advertiser

#
# Provisioning
#
CONFIG_BT_MESH_PB_ADV=y
CONFIG_BT_MESH_UNPROV_BEACON_INT=5
CONFIG_BT_MESH_PB_ADV_TRANS_PDU_RETRANSMIT_COUNT=0
CONFIG_BT_MESH_PB_ADV_TRANS_ACK_RETRANSMIT_COUNT=2
CONFIG_BT_MESH_PB_ADV_LINK_CLOSE_RETRANSMIT_COUNT=2
CONFIG_BT_MESH_PB_ADV_RETRANS_TIMEOUT=500
CONFIG_BT_MESH_PB_GATT=y
CONFIG_BT_MESH_PB_GATT_USE_DEVICE_NAME=y
# CONFIG_BT_MESH_PB_GATT_CLIENT is not set
CONFIG_BT_MESH_PROVISIONEE=y
CONFIG_BT_MESH_PROV_OOB_PUBLIC_KEY=y
CONFIG_BT_MESH_ECDH_P256_CMAC_AES128_AES_CCM=y
CONFIG_BT_MESH_ECDH_P256_HMAC_SHA256_AES_CCM=y
# CONFIG_BT_MESH_OOB_AUTH_REQUIRED is not set
# CONFIG_BT_MESH_PROVISIONER is not set
CONFIG_BT_MESH_CDB=y
CONFIG_BT_MESH_CDB_NODE_COUNT=8
CONFIG_BT_MESH_CDB_SUBNET_COUNT=1
CONFIG_BT_MESH_CDB_APP_KEY_COUNT=1
# end of Provisioning

#
# Network layer
#
CONFIG_BT_MESH_LOOPBACK_BUFS=3
CONFIG_BT_MESH_NETWORK_TRANSMIT_COUNT=2
CONFIG_BT_MESH_NETWORK_TRANSMIT_INTERVAL=20
CONFIG_BT_MESH_MSG_CACHE_SIZE=32
# CONFIG_BT_MESH_RELAY is not set
# end of Network layer

#
# Transport layer
#

#
# Transport SAR configuration
#
CONFIG_BT_MESH_TX_SEG_MSG_COUNT=5
CONFIG_BT_MESH_RX_SEG_MSG_COUNT=5
CONFIG_BT_MESH_SEG_BUFS=64
CONFIG_BT_MESH_RX_SEG_MAX=20
CONFIG_BT_MESH_TX_SEG_MAX=6
CONFIG_BT_MESH_SAR_TX_SEG_INT_STEP=0x05
CONFIG_BT_MESH_SAR_TX_UNICAST_RETRANS_COUNT=0x02
CONFIG_BT_MESH_SAR_TX_UNICAST_RETRANS_WITHOUT_PROG_COUNT=0x02
CONFIG_BT_MESH_SAR_TX_UNICAST_RETRANS_INT_STEP=0x07
CONFIG_BT_MESH_SAR_TX_UNICAST_RETRANS_INT_INC=0x01
CONFIG_BT_MESH_SAR_TX_MULTICAST_RETRANS_COUNT=0x02
CONFIG_BT_MESH_SAR_TX_MULTICAST_RETRANS_INT=0x09
CONFIG_BT_MESH_SAR_RX_SEG_THRESHOLD=0x03
CONFIG_BT_MESH_SAR_RX_ACK_DELAY_INC=0x01
CONFIG_BT_MESH_SAR_RX_SEG_INT_STEP=0x05
CONFIG_BT_MESH_SAR_RX_DISCARD_TIMEOUT=0x01
CONFIG_BT_MESH_SAR_RX_ACK_RETRANS_COUNT=0x00
# end of Transport SAR configuration

CONFIG_BT_MESH_DEFAULT_TTL=7

#
# Replay Protection List
#
CONFIG_BT_MESH_CRPL=10
CONFIG_BT_MESH_RPL_STORAGE_MODE_SETTINGS=y
# end of Replay Protection List
# end of Transport layer

#
# Access layer
#
CONFIG_BT_MESH_ACCESS_LAYER_MSG=y
CONFIG_BT_MESH_MODEL_VND_MSG_CID_FORCE=y
CONFIG_BT_MESH_MODEL_EXTENSIONS=y
CONFIG_BT_MESH_COMP_PAGE_1=y
CONFIG_BT_MESH_MODEL_EXTENSION_LIST_SIZE=10
CONFIG_BT_MESH_COMP_PAGE_2=y
CONFIG_BT_MESH_COMP_PST_BUF_SIZE=100
CONFIG_BT_MESH_ACCESS_DELAYABLE_MSG=y
CONFIG_BT_MESH_ACCESS_DELAYABLE_MSG_CTX_ENABLED=y
CONFIG_BT_MESH_ACCESS_DELAYABLE_MSG_COUNT=4
CONFIG_BT_MESH_ACCESS_DELAYABLE_MSG_CHUNK_SIZE=10
CONFIG_BT_MESH_ACCESS_DELAYABLE_MSG_CHUNK_COUNT=40
CONFIG_BT_MESH_DELAYABLE_PUBLICATION=y
# end of Access layer

#
# Models
#
CONFIG_BT_MESH_CFG_CLI=y
CONFIG_BT_MESH_CFG_CLI_TIMEOUT=5000
# CONFIG_BT_MESH_HEALTH_CLI is not set
CONFIG_BT_MESH_BLOB_SRV=y
CONFIG_BT_MESH_BLOB_SRV_PULL_REQ_COUNT=4
CONFIG_BT_MESH_BLOB_SIZE_MAX=524288
CONFIG_BT_MESH_BLOB_BLOCK_SIZE_MIN=4096
CONFIG_BT_MESH_BLOB_BLOCK_SIZE_MAX=4096
CONFIG_BT_MESH_BLOB_REPORT_TIMEOUT=10
# CONFIG_BT_MESH_BLOB_CLI is not set

#
# BLOB models common configuration
#
CONFIG_BT_MESH_ALIGN_CHUNK_SIZE_TO_MAX_SEGMENT=y
CONFIG_BT_MESH_BLOB_CHUNK_COUNT_MAX=256
# end of BLOB models common configuration

# CONFIG_BT_MESH_BLOB_IO_FLASH is not set
CONFIG_BT_MESH_DFU_SRV=y

#
# Firmware Update model configuration
#
CONFIG_BT_MESH_DFU_FWID_MAXLEN=16
CONFIG_BT_MESH_DFU_METADATA_MAXLEN=32
CONFIG_BT_MESH_DFU_METADATA=y
CONFIG_BT_MESH_DFU_URI_MAXLEN=32
# end of Firmware Update model configuration

# CONFIG_BT_MESH_DFU_SLOTS is not set
# CONFIG_BT_MESH_RPR_SRV is not set
CONFIG_BT_MESH_RPR_AD_TYPES_MAX=8
CONFIG_BT_MESH_RPR_SRV_SCANNED_ITEMS_MAX=4
CONFIG_BT_MESH_RPR_SRV_AD_DATA_MAX=31
CONFIG_BT_MESH_SAR_CFG=y
CONFIG_BT_MESH_SAR_CFG_SRV=y
CONFIG_BT_MESH_SAR_CFG_CLI=y
CONFIG_BT_MESH_OP_AGG=y
CONFIG_BT_MESH_OP_AGG_SRV=y
CONFIG_BT_MESH_OP_AGG_CLI=y
CONFIG_BT_MESH_OP_AGG_CLI_TIMEOUT=10000
CONFIG_BT_MESH_LARGE_COMP_DATA_SRV=y
CONFIG_BT_MESH_MODELS_METADATA_PAGE_LEN=150
# CONFIG_BT_MESH_LARGE_COMP_DATA_CLI is not set
CONFIG_BT_MESH_OD_PRIV_PROXY_CLI=y
CONFIG_BT_MESH_OD_PRIV_PROXY_CLI_TIMEOUT=5000
CONFIG_BT_MESH_SOL_PDU_RPL_CLI=y
CONFIG_BT_MESH_SOL_PDU_RPL_CLI_TIMEOUT=5000
# CONFIG_BT_MESH_BRG_CFG_SRV is not set
CONFIG_BT_MESH_BRG_TABLE_ITEMS_MAX=16
# CONFIG_BT_MESH_BRG_CFG_CLI is not set
# end of Models

#
# Proxy
#
# CONFIG_BT_MESH_GATT_PROXY is not set
CONFIG_BT_MESH_PROXY_CLIENT=y
CONFIG_BT_MESH_SOLICITATION=y
CONFIG_BT_MESH_PROXY_SOLICITATION=y
CONFIG_BT_MESH_SOL_ADV_XMIT=2
# end of Proxy

#
# Beacons
#
# CONFIG_BT_MESH_BEACON_ENABLED is not set
# CONFIG_BT_MESH_PRIV_BEACONS is not set
# end of Beacons

#
# IV Index & Sequence number
#
CONFIG_BT_MESH_IV_UPDATE_TEST=y
CONFIG_BT_MESH_IV_UPDATE_SEQ_LIMIT=0x800000
CONFIG_BT_MESH_IVU_DIVIDER=4
# end of IV Index & Sequence number

# CONFIG_BT_MESH_LOW_POWER is not set
# CONFIG_BT_MESH_FRIEND is not set

#
# Capabilities
#
CONFIG_BT_MESH_SUBNET_COUNT=2
CONFIG_BT_MESH_APP_KEY_COUNT=2
CONFIG_BT_MESH_MODEL_KEY_COUNT=2
CONFIG_BT_MESH_MODEL_GROUP_COUNT=2
CONFIG_BT_MESH_LABEL_COUNT=2
# end of Capabilities

#
# Persistent storage
#
CONFIG_BT_MESH_STORE_TIMEOUT=3
CONFIG_BT_MESH_SEQ_STORE_RATE=128
CONFIG_BT_MESH_RPL_STORE_TIMEOUT=5
CONFIG_BT_MESH_SETTINGS_WORKQ=y
CONFIG_BT_MESH_SETTINGS_WORKQ_PRIO=2
CONFIG_BT_MESH_SETTINGS_WORKQ_STACK_SIZE=768
# end of Persistent storage

CONFIG_BT_MESH_MAX_CONN=2
# CONFIG_BT_MESH_STATISTIC is not set
